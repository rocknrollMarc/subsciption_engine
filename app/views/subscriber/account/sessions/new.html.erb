<h2>Sign in</h2>
<%= form_for @user, :url => sessions_url do |f|  %>
  <%= link_to "New User?", user_signed_up_url  %>
  <p>
    <%= f.label :email  %><br />
    <%= f.email_field :email  %>
  </p>
  <p>
    <%= f.label :password  %><br />
    <%= f.password_field :password  %>
  </p>
  <p>
    <%= f.submit "Sign in"  %>
  </p>
<% end %>
